{
  "name": "@cloudbase/auth",
  "version": "0.1.0-beta.4",
  "description": "cloudbase javascript sdk auth componets",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "tsc": "rm -rf dist/ && tsc -p tsconfig.esm.json && tsc -p tsconfig.json",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "eslint --fix \"./src/**/*.ts\"",
    "build": "npm run lint:fix && npm run tsc",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cloudbase",
    "tcb",
    "javascript",
    "web",
    "sdk",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cloudbase/adapter-interface": "^0.4.0",
    "@cloudbase/types": "^0.1.0-beta.6",
    "@cloudbase/utilities": "^0.1.0-beta.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.7.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@cloudbase/app": "^0.1.0-beta"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  }
}
